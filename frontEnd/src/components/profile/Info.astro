---
interface Props{
    title: string;
    text: string;
}

const { title, text } = Astro.props;

---

<div class="flex flex-col w-full border-b-2 border-b-placeholderBlue py-4">
    <p class="text-subTitle2 font-Cocogoose-SemiLight text-darkBlue">
        { title }
    </p>
    <p class="text-paragraph font-Cocogoose-Light">
        { text } 
    </p>
</div>